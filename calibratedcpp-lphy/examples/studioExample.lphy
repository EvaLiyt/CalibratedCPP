data{
    n = 20;
}
model{
    clade1 = ["leaf_1", "leaf_2", "leaf_3"];
    clade2 = ["leaf_4", "laef_5"];
    age1 ~ Uniform(lower=0.09, upper=0.11);
    age2 ~ Uniform(lower=0.49, upper=0.51);
    tree ~ CalibratedCPP(lambda=2, mu=0.1, rho=1, n=n, cladeTaxa = [clade1, clade2], cladeAge=[age1,age2], rootAge=2);
}