                            <taxon id="leaf_55" spec="Taxon"/>
                        </taxonset>
                        <distr spec="beast.base.inference.distribution.Uniform" lower="1.49" upper="1.51"/>
                </calibrations>
                <calibrations spec="beast.base.evolution.speciation.CalibrationPoint" id="clade4">
                        <taxonset id="clade_4_taxa" spec="TaxonSet">
