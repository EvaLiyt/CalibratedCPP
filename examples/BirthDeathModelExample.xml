        </state>
        <distribution id="posterior" spec="beast.base.inference.CompoundDistribution" >
            <distribution id="birthDeath" spec="calibratedcpp.CalibratedCoalescentPointProcess" tree="@tree" origin="3.0" conditionOnCalibrations="false">
                <treeModel id="treeModel" spec="calibratedcpp.model.BirthDeathModel" birthRate="@birthRate" deathRate="@deathRate" rho="0.1"/>
                <calibrations spec="beast.base.evolution.speciation.CalibrationPoint" id="clade1">
                        <taxonset id="clade_1_taxa" spec="TaxonSet">
                            <taxon id="leaf_1" spec="Taxon"/>
                            <taxon id="leaf_2" spec="Taxon"/>
                        </taxonset>
                        <distr spec="beast.base.inference.distribution.Uniform" lower="0.99" upper="1.01"/>
                </calibrations>
                <calibrations spec="beast.base.evolution.speciation.CalibrationPoint" id="clade2">
                    <taxonset id="clade_2_taxa" spec="TaxonSet">
