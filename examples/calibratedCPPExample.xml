<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beast namespace="beast.pkgmgmt
                 :beast.base.core
                 :beast.base.inference
                 :beast.base.inference.distribution
                 :beast.base.util
                 :beast.base.math
                 :beast.evolution.nuc
                 :beast.base.evolution.operator
                 :beast.base.inference.operator
                 :beast.base.evolution.likelihood
                 :beast.base.evolution.speciation
                 :beast.base.core
                 :beast.base.inference.parameter
                 :beast.base.evolution.operator.kernel
                 :beastlabs.evolution.operators" version="2.0">
<run chainLength="10000" spec="MCMC" storeEvery="1000">
    <state id="state" storeEvery="100000">
        <stateNode id="tree" spec="feast.fileio.TreeFromNexusFile"
                   fileName="./ReMASTER_Sims/simulations_101tip.$(TreeIndex=1).trees"
                   IsLabelledNewick="true"
                   adjustTipHeights="false"
        />
    </state>
    <distribution id="birthDeath" spec="calibratedcpp.evolution.speciation.CalibratedCoalescentPointProcess" conditionOnCalibrations="false">
        <treeModel spec="calibratedcpp.BirthDeathModel" origin="5.0">
            <reproductiveNumber value="2.0"/>
            <deathRate value="1.0"/>
            <rho value="0.1"/>
        </treeModel>
        <calibrations>
            <CalibrationPoint taxa=""/>

        </calibrations>
    </distribution>
</run>
</beast>